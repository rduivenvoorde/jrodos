<?xml version = "1.0"?>
<?xml-stylesheet type="text/xsl" href="plugins.xsl"?>
 <plugins>

   <pyqgis_plugin name="RIVM SectorPlot" version="1.1.5">
     <download_url>http://repo.svc.cal-net.nl/repo/rivm/qgis/SectorPlot.1.1.5.zip</download_url>
     <description>Plots sector areas on risk management map</description>
     <homepage>https://git.svc.cal-net.nl/qgis/sectorplotplugin</homepage>
     <qgis_minimum_version>3.4</qgis_minimum_version>
     <!--<file_name>SectorPlot.zip</file_name>-->
     <author_name>RIVM, Marnix de Ridder</author_name>
   </pyqgis_plugin>

   <pyqgis_plugin name="RIVM JRodos" version="2.1.1">
     <download_url>http://repo.svc.cal-net.nl/repo/rivm/qgis/JRodos.2.1.1.zip</download_url>
     <description>This plugin shows JRodos model output, measurements and weather data</description>
     <homepage>https://git.svc.cal-net.nl/qgis/jrodos</homepage>
     <qgis_minimum_version>3.16</qgis_minimum_version>
     <!--<file_name>JRodos.zip</file_name>-->
     <author_name>RIVM, Marnix de Ridder</author_name>
   </pyqgis_plugin>

   <pyqgis_plugin name="RIVM Meetwagen Plugin" version="0.4.0">
     <download_url>http://repo.svc.cal-net.nl/repo/rivm/qgis/meetwagen.0.4.0.zip</download_url>
     <description>This plugin shows the active vehicles and the WVI layers</description>
     <homepage>https://git.svc.cal-net.nl/qgis/meetwagenplugin</homepage>
     <qgis_minimum_version>3.16</qgis_minimum_version>
     <!--<file_name>JRodos.zip</file_name>-->
     <author_name>RIVM, Marnix de Ridder</author_name>
   </pyqgis_plugin>

   <pyqgis_plugin name="RIVM PluginConfigManager" version="1.1.0">
     <download_url>http://repo.svc.cal-net.nl/repo/rivm/qgis/RIVM_PluginConfigManager.1.1.0.zip</download_url>
     <description>This plugin is used to configure RIVM plugins</description>
     <homepage>https://git.svc.cal-net.nl/qgis/configuratie</homepage>
     <qgis_minimum_version>3.16</qgis_minimum_version>
     <!--<file_name>RIVM_PluginConfigManager.zip</file_name>-->
     <author_name>RIVM, Marnix de Ridder</author_name>
   </pyqgis_plugin>

